{
	"scripts": {
		"build": "npm run clean && webpack --env production",
		"clean": "rimraf public/scripts/* public/styles/* public/index.html",
		"dev": "concurrently --kill-others \"webpack\" \"firebase emulators:start\" \"npm run serve\" \"webpack --stats --json=webpack.stats.json\"",
